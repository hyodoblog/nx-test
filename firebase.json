{
  "functions": {
    "runtime": "nodejs16",
    "source": "dist/apps/functions",
    "ignore": [
      "**/.env.*",
      "**/.prettier*",
      "**/.gitignore",
      "**/.runtimeconfig*",
      "**/*.log",
      "**/README.md",
